<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(EndDate)) , NOT(ISBLANK(EffectiveDate)),
EndDate&lt;EffectiveDate)</errorConditionFormula>
    <errorDisplayField>EndDate</errorDisplayField>
    <errorMessage>End Date should be greater than Start Date</errorMessage>
</ValidationRule>
